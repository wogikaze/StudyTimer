{
  "configVersion": "v3",
  "app": {
    "appType": "app",
    "version": {
      "code": 1,
      "name": "1.0.0"
    },
    "appId": 20424,
    "appName": "Study Timer",
    "icon": "icon.png",
    "vender": "zepp",
    "description": "todo list application"
  },
  "runtime": {
    "apiVersion": {
      "compatible": "3.0.0",
      "target": "3.0.0",
      "minVersion": "3.0"
    }
  },
  "targets": {
    "gt": {
      "module": {
        "page": {
          "pages": [
            "page/home/index.page",
            "page/home/timer"
          ]
        },
        "app-side": {
          "path": "app-side/index"
        },
        "setting": {
          "path": "setting/index"
        },
        "app-service": {
          "services": ["app-service/background_service"]
        }
      },
      "platforms": [
        {
          "st": "r",
          "dw": 480
        },
        {
          "st": "s",
          "dw": 390
        }
      ]
    }
  },
  "permissions": [
    "data:os.device.info",
    "device:os.local_storage"
  ],
  "i18n": {
    "en-US": {
      "name": "Todo List"
    }
  },
  "defaultLanguage": "en-US",
  "debug": false
}
